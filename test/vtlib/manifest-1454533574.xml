  `registrodeventasid` int(19) DEFAULT NULL,
  `localizadorid` int(11) DEFAULT NULL,
  `monto_base` double(8,2) DEFAULT NULL,
  `fecha_emision` date DEFAULT NULL,
  `passenger` varchar(100) DEFAULT NULL,
  `itinerario` varchar(250) DEFAULT NULL,
  `status` varchar(50) DEFAULT NULL,
  `tipodevuelo` varchar(50) DEFAULT NULL,
